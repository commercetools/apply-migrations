{
  "name": "ct-migrate",
  "version": "0.1.0",
  "description": "A tool for applying scripted changes to commercetools projects.",
  "main": "dist/index.js",
  "watch": {
    "dev": "src"
  },
  "scripts": {
    "babel": "babel src -d dist",
    "lint": "eslint src --fix",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run babel",
    "dev": "npm run lint && npm run build && npm run start",
    "start": "node dist/index.js",
    "jest": "jest",
    "test": "npm run build && npm run jest",
    "commitmsg": "validate-commit-msg -p jshint",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "keywords": [
    "commercetools",
    "node",
    "migrations"
  ],
  "author": "commercetools",
  "license": "MIT",
  "repository": "",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.1",
    "lint-staged": "^6.0.1",
    "nock": "^9.1.6",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.1",
    "validate-commit": "^3.4.0"
  },
  "dependencies": {
    "@commercetools/api-request-builder": "^3.5.0",
    "@commercetools/sdk-client": "^1.5.3",
    "@commercetools/sdk-middleware-auth": "^3.4.4",
    "@commercetools/sdk-middleware-http": "^2.2.3",
    "@commercetools/sdk-middleware-queue": "^1.1.3",
    "commander": "^2.17.1",
    "winston": "^3.0.0-rc5"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
